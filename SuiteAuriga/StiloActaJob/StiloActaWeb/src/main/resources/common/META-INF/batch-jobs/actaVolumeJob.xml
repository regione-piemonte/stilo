<?xml version="1.0" encoding="UTF-8"?>
<job id="actaVolumeJob" xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="1.0">

    <!-- Close Volumes -->
    <step id="actaVolumeClosingStep" next="actaVolumeOpeningStep">
        <batchlet ref="actaVolumeClosingBatchlet"/>
    </step>

    <!-- Open Volumes -->
    <step id="actaVolumeOpeningStep">
        <batchlet ref="actaVolumeOpeningBatchlet"/>
    </step>

</job>
