<jboss-deployment-structure>
        <deployment>
            <dependencies>
                        <module name="org.apache.cxf.impl" export="true">
                                <imports>
                                        <include path="META-INF" />
                    <include path="META-INF/cxf" />
                </imports>
                <exports>
                   <include path="META-INF" />
                   <include path="META-INF/cxf" />
               </exports>
            </module>
             <module name="org.springframework.spring" export="true">
             <imports>
                 <include path="META-INF" />
             </imports>
             <exports>
               <include path="META-INF" />
             </exports>
            </module>
           </dependencies>
          </deployment>
</jboss-deployment-structure>
