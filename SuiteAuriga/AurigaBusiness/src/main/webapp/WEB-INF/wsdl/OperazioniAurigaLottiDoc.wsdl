						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType name="SignatureType">
				<xs:annotation>
					<xs:documentation>Tipologie di firme ammesse</xs:documentation>
				</xs:annotation>
